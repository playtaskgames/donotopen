{
    "version": 2,
    "builds": [
        { "src": "public/**/*", "use": "@vercel/static" },
        { "src": "api/*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/fetchFollowers", "dest": "/api/fetchFollowers.js" },
        { "src": "/script.js", "dest": "/script.js" },
        { "src": "/(.*)", "dest": "/public/$1" }
    ]
}